    <rr:ObjectiveHeading contextRef="doc_Institutional_Shares_ProspectusMember_S000021482Member">
        
          
            Investment Objective
          
        
      </rr:ObjectiveHeading>
    <rr:ObjectivePrimaryTextBlock contextRef="doc_Institutional_Shares_ProspectusMember_S000021482Member">
        &lt;p style="line-height: normal"&gt;
          &lt;span style="font-size: 10.0pt;"&gt;The fund seeks as high a level of current income exempt
            from federal, New York state and New York city personal income taxes as is consistent
            with the preservation of capital and the maintenance of liquidity.&lt;/span&gt;
        &lt;/p&gt;
      </rr:ObjectivePrimaryTextBlock>
    <rr:ExpenseHeading contextRef="doc_Institutional_Shares_ProspectusMember_S000021482Member">
        
          
            Fees and Expenses
          
        
      </rr:ExpenseHeading>
